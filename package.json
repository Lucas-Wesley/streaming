{
  "name": "streaming",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:build": "docker build -t str_backend ./docker",
    "docker:run": "docker run -it -v $(pwd)/.:/app str_backend bash",
    "compose:up": "docker compose -f docker/docker-compose.yaml up -d --build",
    "compose:down": "docker compose -f docker/docker-compose.yaml down -v",
    "compose:logs": "docker compose -f docker/docker-compose.yaml logs -f",
    "compose:logs:monitor": "docker compose -f docker/docker-compose.yaml logs -f monitor",
    "compose:exec": "docker compose -f docker/docker-compose.yaml exec -it backend bash",
    "compose:dev": "docker compose -f docker/docker-compose.yaml exec -it backend npm run dev",
    "compose:restart": "npm run compose:down && npm run compose:up",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "nodemon --exec tsx src/api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/express": "^5.0.6",
    "axios": "^1.13.4",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "nodemon": "^3.1.11",
    "pg-promise": "^12.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
